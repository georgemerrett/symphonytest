{# templates/user/view.html.twig #}

<div class="container table-user">
        
        <table class='table table-striped'>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Date of Birth</th>
          <th>Email Address</th>
        </tr>
        {% for item in users %}
        <tr>
          <td>{{item.firstname}}</td>
          <td>{{item.lastname}}</td>
          <td>{{item.birthdate}}</td>
          <td>{{item.email}}</td>
        </tr>
        {% endfor %}
        </table>

</div>
